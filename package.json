{
  "name": "@festicket/route-manager",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "postbuild": "flow-copy-source src dist",
    "lint:js": "eslint ./src/**/* --ext .js --ext .jsx --fix",
    "lint": "npm run lint:js && npm run flow",
    "flow": "flow",
    "glow": "glow",
    "flow:libdefs": "flow gen-flow-files src/ --out-dir dist/",
    "flow:typed": "npx flow-typed install --overwrite --ignoreDeps peer",
    "test": "jest",
    "start": "babel-node src/index.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "npm run flow && lint-staged",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "esm": "^3.0.2",
    "path-to-regexp": "^2.2.0",
    "query-string": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-angular": "^6.0.2",
    "@festicket/eslint-config": "^1.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-import-resolver-jest": "^1.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "flow-bin": "^0.63.1",
    "flow-copy-source": "^1.3.0",
    "flow-typed": "^2.3.0",
    "folder-module": "^2.0.4",
    "glow": "^1.2.2",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.0.1",
    "prettier": "^1.11.1",
    "semantic-release": "^15.12.2"
  },
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 ChromeAndroid versions",
    "Edge >= 14",
    "Explorer >= 11",
    "last 3 Firefox versions",
    "iOS >= 8",
    "Safari >= 9"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/festicket/route-manager.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
