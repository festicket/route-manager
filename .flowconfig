[ignore]
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/node_modules/config-chain/.*
<PROJECT_ROOT>/node_modules/npm/node_modules/config-chain/.*
<PROJECT_ROOT>/commitlint\.config\.js
<PROJECT_ROOT>/jest\.config\.js

[include]

[libs]

[lints]
all=warn
unclear-type=off
sketchy-null=off

[options]
module.name_mapper='^@festicket\/route-manager$' -> '<PROJECT_ROOT>/src/'
module.name_mapper='^src/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
include_warnings=true

[strict]
